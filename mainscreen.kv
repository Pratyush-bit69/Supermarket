#: import FadeTransition kivy.uix.screenmanager.FadeTransition

<IconListItem>

<ContentNavigationDrawer>
    MDList:
        OneLineListItem:
            text: "Home"
            on_press:
                root.nav_drawer.set_state("close")
                root.screen_manager.transition=FadeTransition()
                root.screen_manager.current = "mainscreen"
        OneLineListItem:
            text: "Transaction"
            on_press:
                root.nav_drawer.set_state("close")
                root.screen_manager.transition=FadeTransition()
                root.screen_manager.current = "transaction"
        OneLineListItem:
            text:"Product"
            on_press:
                root.nav_drawer.set_state("close")
                root.screen_manager.transition=FadeTransition()
                root.screen_manager.current = "product"
        OneLineListItem:
            text:"Employee"
            on_press:
                root.nav_drawer.set_state("close")
                root.screen_manager.transition=FadeTransition()
                root.screen_manager.current = "employee"

        OneLineListItem:
            text:"Customer"
            on_press:
                root.nav_drawer.set_state("close")
                root.screen_manager.transition=FadeTransition()
                root.screen_manager.current = "customer"
        OneLineListItem:
            text:"Promo"
            on_press:
                root.nav_drawer.set_state("close")
                root.screen_manager.transition=FadeTransition()
                root.screen_manager.current = "promo"

